!cpu 6502
!to "build/PRELAUNCH.INDEXED/CEILING.ZERO",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $083A
         jsr   $0800      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $902B      ; patch - don't decrease lives
         sta   $77ED      ; patch - don't increase lives
+
         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jmp   $5FED

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
