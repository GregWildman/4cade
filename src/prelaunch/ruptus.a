!cpu 6502
!to "build/PRELAUNCH.INDEXED/RUPTUS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $813
         jsr   $800       ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $40A4      ; patch - don't decrease lives
         sta   $611E      ; patch - don't increase lives
+

         +DISABLE_ACCEL
         jmp   $4000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}
