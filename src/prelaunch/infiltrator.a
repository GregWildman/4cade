!cpu 6502
!to "build/PRELAUNCH.INDEXED/INFILTRATOR",plain
*=$106

    !source "src/macros.a"

         +READ_RAM2_WRITE_RAM2
         +LC_REBOOT
         jsr   HideLaunchArtworkLC2
         +READ_ROM_NO_WRITE
         lda   #$B4
         sta   $98E
         lda   #$EF
         sta   $98D      ; reset vector fix

         jmp   $800

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
