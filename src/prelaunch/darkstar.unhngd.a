!cpu 6502
!to "build/PRELAUNCH.INDEXED/DU",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $5F28
         jsr   $800       ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $9AB3      ; patch - don't decrease lives
+
         +DISABLE_ACCEL
         jmp   $800

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
