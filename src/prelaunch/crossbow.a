!cpu 6502
!to "build/PRELAUNCH.INDEXED/CROSSBOW",plain
*=$106

         !source "src/macros.a"

         lda   #$60
         sta   $33C
         jsr   $300
         +ENABLE_ACCEL
         +LC_REBOOT
         lda   #$60
         sta   $9A1
         jsr   $800       ; decompress
         stx   $827F
         inx
         stx   $828A      ; reset vector
         lda   #$4C
         sta   $7144
         jsr   $68FD
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$60
         sta   $7E46      ; no collisions
+
         jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         +READ_ROM_NO_WRITE
         jmp   $7147

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
