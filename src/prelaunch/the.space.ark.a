!cpu 6502
!to "build/PRELAUNCH.INDEXED/THE.SPACE.ARK",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL

         +NEW_RESET_VECTOR reset

         lda   #$60
         sta   $A2C
         jsr   $800       ; load "DOS"
         lda   #$60
         sta   $B7A7
         jsr   $B700      ; decompress

         +DISABLE_ACCEL_AND_HIDE_ARTWORK

         lda   #0
         jmp   $B7A9

reset

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
