!cpu 6502
!to "build/PRELAUNCH.INDEXED/AO",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $603F
         jsr   $6000      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $0E55
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $4D00

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
