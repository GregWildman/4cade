!cpu 6502
!to "build/PRELAUNCH.INDEXED/DJ",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $202F
         jsr   $2000      ; decompress
         lda   #$88
         sta   $E001
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $EE5       ; patch - don't increase lives
         sta   $1226      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         jmp   $200

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
