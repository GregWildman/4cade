!cpu 6502
!to "build/PRELAUNCH.INDEXED/AGENT.U.S.A",plain
*=$106

    !source "src/macros.a"

         lda   #$60
         sta   $36D3
         jsr   $366D      ; decompress

         ldx   #5
-        lda   $100,x
         sta   $280,x
         dex
         bpl   -
         sec
         ror   $3F2
         inc   $3F3       ; point to $280 instead
         jsr   $FB6F

         jsr   HideLaunchArtwork
         jmp   $837

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
