!cpu 6502
!to "build/PRELAUNCH.INDEXED/YEWDOW",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$4C
         sta   $99CD
         jsr   $8000      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $4882      ; patch - don't decrease lives
         sta   $4866      ; patch - don't decrease time
+
         lda   #$80
         sta   $6a68      ; fix joystick down
         +DISABLE_ACCEL
         jmp   $99D0

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
