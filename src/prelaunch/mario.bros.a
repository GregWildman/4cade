!cpu 6502
!to "build/PRELAUNCH.INDEXED/MB",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $822E
         jsr   $4900      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a9
         sta   $4299
+
         lda   #1
         sta   $402F      ; reset vector patch

         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $4000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
