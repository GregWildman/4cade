!cpu 6502
!to "build/PRELAUNCH.INDEXED/SS2",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         +NEW_RESET_VECTOR_64K $70

         lda   #$60
         sta   $DBE
         sta   $DE3
         jsr   $C24
         jsr   $BCFC      ; decompress
         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jsr   $DC1
         +ENABLE_ACCEL
         jsr   $BCFC      ; decompress
         +DISABLE_ACCEL
         jmp   $DE6

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
