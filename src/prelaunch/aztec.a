!cpu 6502
!to "build/PRELAUNCH.INDEXED/AZTEC",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1F3C
         jsr   $1E60      ; decompress
         lda   #$20
         sta   $6571
         lda   #<keypress
         sta   $6572
         lda   #>keypress
         sta   $6573
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $61B5
         sta   $61BD
         sta   $61C3
         sta   $61D0
         sta   $52D7
         sta   $144B
+
         +DISABLE_ACCEL
         jmp   $1B00

keypress
         lda   $C000
         bpl   keypress
         cmp   #$8D
         bne   keypress
         lda   #$8D
         rts

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
