!cpu 6502
!to "build/PRELAUNCH.INDEXED/LUNAR.EXPLORER",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         lda   #$60
         sta   $83A
         jsr   $800        ; decompress
         ldx   #0
         stx   $66F4
         inx
         stx   $66F9
         lda   #$A4
         sta   $66FE
         +DISABLE_ACCEL
         jmp   $65E2

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
