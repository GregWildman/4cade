!cpu 6502
!to "build/PRELAUNCH.INDEXED/AXIS.ASSASSIN",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3f32
         sta   $3f4e
         jsr   HideLaunchArtwork
         jsr   $3efc      ; decompress & show title
         +DISABLE_ACCEL
         jsr   $F00
         +ENABLE_ACCEL
         jsr   $be12      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6d31
+
         lda   #$4C
         sta   $5B2E
         ldx   #0
         stx   $5B2F
         inx
         stx   $5B30      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $5c00

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
