!cpu 6502
!to "build/PRELAUNCH.INDEXED/BOA",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $835
         jsr   $800       ; decompress
         sta   $299
         lda   #1
         sta   $F0
         lsr
         sta   $F1
         lda   #$0A
         sta   $F2
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   ($6000)

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
