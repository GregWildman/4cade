!cpu 6502
!to "build/PRELAUNCH.INDEXED/HELI.RESCUE",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         +NEW_RESET_VECTOR reset

         lda   #<callback
         sta   $1E36
         lda   #>callback
         sta   $1E37
         jmp   $1D7C      ; decompress

callback
         +DISABLE_ACCEL
         jmp   $17B4
reset

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}
