!cpu 6502
!to "build/PRELAUNCH.INDEXED/SD",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $0853
         jsr   $0800      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
                          ; lives in $17
         lda   #$A5
         sta   $6D8A      ; patch - don't decrease lives
         sta   $6DDA      ; patch - don't increase lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $6000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
