!cpu 6502
!to "build/PRELAUNCH.INDEXED/BUG.BATTLE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9468
         jsr   $6400      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$bd
         sta   $78F2      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         +READ_ROM_NO_WRITE
         jmp   $447

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
