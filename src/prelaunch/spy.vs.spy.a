!cpu 6502
!to "build/PRELAUNCH.INDEXED/SS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4043
         sta   $406A
         sta   $408E
         jsr   $4000
         jsr   $4097      ; decompress
         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jsr   $4046
         +ENABLE_ACCEL
         jsr   $4097      ; decompress
         jsr   $406D      ; decompress
         lda   #1
         sta   $6010
         lda   #$A4
         sta   $6011      ; reset vector
         +DISABLE_ACCEL
         lda   #$5F
         jmp   $4090

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
