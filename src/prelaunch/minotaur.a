!cpu 6502
!to "build/PRELAUNCH.INDEXED/MINOTAUR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $21BB
         jsr   $2000      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$37
         sta   $180       ; patch - enable cheat mode
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $FF8

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
