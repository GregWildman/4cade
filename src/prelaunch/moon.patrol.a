!cpu 6502
!to "build/PRELAUNCH.INDEXED/MOON.PATROL",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $49dd
         jsr   $800

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $2d07
         sta   $1c17
+
         +NEW_RESET_VECTOR $280

         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $919

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
