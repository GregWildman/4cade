!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPACE.EGGS",plain
*=$106

    !source "src/macros.a"

         +NEW_RESET_VECTOR $280
         +ENABLE_ACCEL

         lda   #$60
         sta   $2056
         jsr   $2000      ; decompress

         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jmp   $4880      ; run it

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
