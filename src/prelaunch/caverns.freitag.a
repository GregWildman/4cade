!cpu 6502
!to "build/PRELAUNCH.INDEXED/CF",plain
*=$106

    !source "src/macros.a"

         jsr   HideLaunchArtwork
         ;+ENABLE_ACCEL   ; shows title

         +NEW_RESET_VECTOR $280

         lda   #$60
         sta   $40B1
         jsr   $4000
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$30
         sta   $60C6
         sta   $60C7      ; patch - don't decrease hits
+
         jmp   $D566

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
