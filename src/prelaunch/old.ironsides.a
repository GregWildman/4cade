!cpu 6502
!to "build/PRELAUNCH.INDEXED/OLD.IRONSIDES",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1E84
         lda   #<callback1
         sta   $1EA0
         lda   #<callback2
         sta   $1EB7
         lda   #>callback1
         sta   $1EA1
         sta   $1EB8
         jsr   $1E00       ; decompress
         +ENABLE_ACCEL
         jmp   $120C

callback1
         jsr   $BE23       ; preserve $7FE for Laser
         +ENABLE_ACCEL
         rts

callback2
         +DISABLE_ACCEL
         jmp   $BE23

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
