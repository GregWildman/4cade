!cpu 6502
!to "build/PRELAUNCH.INDEXED/BR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4083
         jsr   $4000      ; decompress
         lda   $7fe       ; Buck Rogers code
         pha
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         pla
         sta   $7fe       ; overwritten by GS acceleration
         jmp   $400

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
