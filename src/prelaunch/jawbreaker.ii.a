!cpu 6502
!to "build/PRELAUNCH.INDEXED/JAWBREAKER.II",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $85B
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #<callback
         sta   $35C
         lda   #>callback
         sta   $35D
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $301

callback
         ldx   #$a5
         stx   $110c      ; patch - don't decrease lives
         jmp   $BD00

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
