!cpu 6502
!to "build/PRELAUNCH.INDEXED/MEGABOTS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9F1
         jsr   $800       ; load DOS

         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         ldx   #$60
         jmp   $B700

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
