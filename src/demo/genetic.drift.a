!cpu 6502
!to "res/DEMO/GENETIC.DRIFT",plain
*=$300

         !source "src/constants.a"   ; no code in these
         !source "src/macros.a"

         +ENABLE_ACCEL
         +LOAD_XSINGLE title

         lda   #$60
         sta   $846
         jsr   $800       ; decompress
         lda   #$60
         sta   $8439
         lda   #$4C
         sta   $FC9
         ldx   #0
         stx   $FCA
         inx
         stx   $FCB
         jsr   $8490      ; decompress more

         +DISABLE_ACCEL

         jsr   $F00
         jmp   $100

title    !byte x_e-x_b
x_b      !text "GENETIC.DRIFT"
x_e

!if * > $3F0 {
  !error "code is too large, ends at ", *
}
