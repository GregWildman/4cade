!cpu 6502
!to "res/DEMO/SD",plain
*=$300

         !source "src/constants.a"   ; no code in these
         !source "src/macros.a"

         +READ_RAM2_WRITE_RAM2
         jsr   EnableAccelerator
         +LOAD_FILE_KEEP_DIR sd, sd_dir_e-sd_dir_b
         lda   #$60
         sta   $0853
         jsr   $0800      ; decompress
         lda   #$4C
         sta   $60D1
         sta   $69B9
         ldx   #0
         stx   $60D2
         stx   $69BA
         stx   $6D71
         stx   $72F3
         inx
         stx   $60D3
         stx   $69BB
         stx   $6D72
         asl   $7324
         inc   $7325
         lda   #$15
         sta   $732A
         inc   $732B
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $6000

sd
         !byte sd_e-sd_b
sd_b
sd_dir_b
         !text "X/SD"
sd_dir_e
         !text "/SD"
sd_e

!if * > $3F0 {
  !error "code is too large, ends at ", *
}
